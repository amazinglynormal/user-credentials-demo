{
  "name": "user-credentials-demo",
  "version": "1.0.0",
  "description": "Combining Spring and Reactjs, allows user to sign up, log in, change details and delete account.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack.config.js --node-env development",
    "build": "webpack --config webpack.config.js --node-env production",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch --env=jsdom",
    "lint": "eslint --fix \"./frontend/src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./frontend/src/**/*.{js,jsx,ts,tsx,json,css,scss,md} \""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amazinglynormal/user-credentials-demo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amazinglynormal/user-credentials-demo/issues"
  },
  "homepage": "https://github.com/amazinglynormal/user-credentials-demo#readme",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.2.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "style-loader": "^3.2.1",
    "webpack": "^5.47.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.8",
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/frontend/src/setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/frontend/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
